{
  "name": "go-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build-storybook": "storybook build",
    "build": "next build",
    "build:dev": "NEXT_PUBLIC_APP_URL=http://localhost.docker:3000 next build",
    "ci-check": "yarn typecheck && yarn lint && yarn format:check && yarn test:unit:once",
    "codegen:all-rest-services": "orval",
    "codegen:covers": "rm -rf src/lib/rest/cover-service-api/generated/model/*.* && orval --project coverService",
    "codegen:graphql": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --require tsconfig-paths/register --config codegen.ts",
    "codegen:publizon": "rm -rf lib/rest/publizon-api/generated/model/*.* && orval --project publizonAdapter",
    "dev:https": "NEXT_PUBLIC_APP_URL=https://dapple-cms.docker:3000 NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -H dapple-cms.docker --experimental-https",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "next lint",
    "start:with-server-source-maps": "NODE_OPTIONS='--enable-source-maps=true' next start",
    "start": "next start",
    "start:dev": "NEXT_PUBLIC_APP_URL=http://localhost.docker:3000 NODE_TLS_REJECT_UNAUTHORIZED=0 next start",
    "storybook": "storybook dev -p 6006",
    "test:accessibility": "test-storybook",
    "test:unit:once": "vitest run",
    "test:unit": "vitest",
    "typecheck": "tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "shadcn:add": "npx shadcn@canary add $MODULE",
    "post-process-dpl-cms-graphql": "NODE_OPTIONS='--loader=ts-node/esm' ts-node ./graphql-codegen-scripts/post-process-dpl-cms-graphql.ts"
  },
  "dependencies": {
    "@next/env": "15.0.3",
    "@plaiceholder/next": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@redux-devtools/extension": "^3.3.0",
    "@tanstack/react-query": "^5.59.0",
    "@types/lodash": "^4.17.12",
    "@uidotdev/usehooks": "^2.4.1",
    "@xstate/react": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "12.0.0-alpha.1",
    "graphql": "^16.9.0",
    "iron-session": "^8.0.3",
    "keen-slider": "^6.8.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.476.0",
    "next": "^15.1.6",
    "next-drupal": "^2.0.0-beta.0",
    "next-themes": "^0.4.4",
    "openid-client": "^6.1.3",
    "plaiceholder": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-parallax-tilt": "^1.7.252",
    "replace-in-file": "^8.3.0",
    "sharp": "^0.33.5",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "vaul": "^1.1.1",
    "xstate": "^5.18.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@chromatic-com/storybook": "^3.2.4",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@next/eslint-plugin-next": "15.0.3",
    "@parcel/watcher": "^2.4.1",
    "@storybook/addon-a11y": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/nextjs": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@storybook/test-runner": "^0.21.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.7",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/create-react-app": "^2.0.5",
    "@types/node": "^20.0.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@vitejs/plugin-react": "^4.3.2",
    "axe-playwright": "^2.0.3",
    "cypress": "^13.16.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.10.1",
    "jsdom": "^25.0.1",
    "next-test-api-route-handler": "^4.0.12",
    "node-mocks-http": "^1.16.1",
    "orval": "^7.2.0",
    "postcss": "^8.5.3",
    "prettier": "^3.3.3",
    "prettier-plugin-classnames": "^0.7.3",
    "prettier-plugin-merge": "^0.7.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.5.3",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "resolutions": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
